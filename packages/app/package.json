    "@sveltejs/kit": "^1.24.1",
    "@unocss/reset": "^0.55.7",
    "@urql/core": "^4.1.2",
    "d3": "^7.8.5",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.33.0",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-css-order": "^1.3.1",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "typescript": "^5.2.2",
